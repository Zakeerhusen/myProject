<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog title="Delete user {/Firstname} {/Lastname} ?"
		>
		<!-- <f:SimpleForm editable="true"> -->
			<Label text="Are you sure want to delete this user?"/>
		<!-- </f:SimpleForm> -->
		<beginButton>
			<Button text="Yes" press="onDeleteUser" />
		</beginButton>
		<endButton>
			<Button text="Cancel" press="closeDeleteUser" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>